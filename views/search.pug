extends layouts/main
include mixins/videoBlock  


block content
    .search__header
        h3 Searching for: #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos 

            //- mixin을 호출
            +videoBlock({ 
                title:item.title,
                views:item.views,
                videoFile:item.videoFile,
                id:item.id  //- ID를 videoBlock에 할당
            })  
               